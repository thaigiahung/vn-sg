<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">New Order</h1>
  </div>
  <!-- /.col-lg-12 -->
</div>
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        List of new order
      </div>
      <!-- /.panel-heading -->
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped table-bordered table-hover" id="tableNewOrder">
            <thead>
              <tr>
                <th>Id</th>
                <th>Receiver</th>
                <th>Address</th>
                <th>Phone</th>
                <th>Total</th>
              </tr>
            </thead>
            <tbody>
              <% _.each(data, function (obj) { %>
              <tr>
                <td>
                  <a href="/order/<%= obj.id %>/detail"><%= obj.id %></a>
                </td>
                <td>
                  <a href="/order/<%= obj.id %>/detail"><%= obj.receiver %></a>
                </td>
                <td>
                  <a href="/order/<%= obj.id %>/detail"><%= obj.address %></a>
                </td>
                <td>
                  <a href="/order/<%= obj.id %>/detail"><%= obj.phone %></a>
                </td>
                <td>
                  <a href="/order/<%= obj.id %>/detail"><%= obj.total %> SGD</a>
                </td>                           
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
      </div>
      <!-- /.panel-body -->
    </div>
    <!-- /.panel -->

  </div>
  <!-- /.col-lg-12 -->
</div>